
<div id='flash_bar' class='row'>
    <% if notice %>
        <p class='alert alert-info'>
          <button type='button' class='close' data-dismiss='alert' aria-hidden='true'>&times;</button>
          <%= notice %>
        </p>
    <% end %>
    <% if alert %>
        <p class='alert alert-danger'><%= alert %></p>
    <% end %>
</div>